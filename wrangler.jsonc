			"name": "mcp-oauth-authorization-server-staging",
			"d1_databases": [
				{
					"binding": "AUTH_DB",
					"database_name": "mcp-oauth-db-staging",
					"database_id": "471fa3ec-770b-4bbf-87fb-4f565e3f3c1b"
				}
			]
		}
	}
}